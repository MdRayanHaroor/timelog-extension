{
  "manifest_version": 3,
  "name": "Developer Time Logger",
  "version": "1.0",
  "description": "Log and track time spent on development tasks",
  "permissions": ["storage"],
  "host_permissions": ["https://dev.azure.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Developer Time Logger",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
